<!DOCTYPE html>
<html>

<head>
  <title>
    User List
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <h1>All Users</h1>
  <h2>
    <ul>
      <% users.forEach(function(user){ %>
        <li>
          <%= user.id %>
          <%= user.name %>
          <%= user.email %>
        </li>
      <% }); %>
    </ul>
  </h2>
</body>

</html>